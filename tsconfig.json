{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "useDefineForClassFields": true,
    "baseUrl": "./src",
    "paths": {
      "@components": [
        "components"
      ],
      "@features/*": [
        "features/*"
      ],
      "@navigation/*": [
        "navigation/*"
      ],
      "@utils": [
        "utils"
      ],
      "@store/*": [
        "store/*"
      ],
      "@types/*": [
        "types/*"
      ],
    }
  }
}